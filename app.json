{
  "expo": {
    "name": "OutDrinkMe",
    "slug": "outDrinkMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "outdrinkme",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.martbul.outDrinkMe",
      "associatedDomains": [
        "applinks:clerk.martbul.com"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "OutDrinkMe needs camera access to scan beverage barcodes for your collection.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/66padicon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "package": "com.martbul.outDrinkMe",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "outdrinkme"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "clerk.martbul.com",
              "pathPrefix": "/v1/oauth_callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "name": "OutDrinkMe",
      "shortName": "OutDrinkMe",
      "display": "standalone",
      "backgroundColor": "#000000",
      "themeColor": "#000000",
      "startUrl": "/",
      "meta": {
        "apple": {
          "mobileWebAppCapable": "yes",
          "statusBarStyle": "black-translucent"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you share images.",
          "cameraPermission": "The app needs access to your camera to take photos."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow OutDrinkMe to use your location to tag your drinking spots and find nearby venues."
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-1167503921437683~3406815046",
          "iosAppId": "ca-app-pub-1167503921437683~3406815046"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow OutDrinkMe to access camera to scan beverage barcodes."
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": true
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c9bec799-f894-4acc-ad04-3217517dc5cc"
      }
    },
    "owner": "martbul",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c9bec799-f894-4acc-ad04-3217517dc5cc"
    }
  }
}